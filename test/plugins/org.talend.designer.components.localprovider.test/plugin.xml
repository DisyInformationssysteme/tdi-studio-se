<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="Talend"
            id="testComponents"
            name="Refresh templates"/>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="testComponents"
            contextId="talend.global"
            schemeId="org.talend.core.scheme"
            sequence="CTRL+F10"/>
   </extension>
   <extension
         point="org.talend.core.global_actions">
      <GlobalAction class="org.talend.designer.components.localprovider.test.TestComponentsAction"/>
   </extension>
   <extension
         point="org.talend.core.migrationTask">
      <projecttask
        beforeLogon="true"
        breaks="8.0.0"
        class="org.talend.designer.components.localprovider.test.FakeLazyMigrationTask"
        description="add an addtional property: lazymigration=executed for test purpose."
        id="org.talend.designer.components.localprovider.test.FakeLazyMigrationTask"
        name="FakeLazyMigrationTask"
        version="8.0.0">
      </projecttask>
   </extension>
</plugin>
